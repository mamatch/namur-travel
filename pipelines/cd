pipeline {
    agent any

    stages {

        stage('Build'){
            steps {
                withMaven(maven: 'maven-3.6.3'){
                    sh'mvn clean install'
                }
            }
        }

        stage('Test'){
            steps {
                withMaven(maven: 'maven-3.6.3') {
                    sh'mvn test'
                }

            }
        }

    }
}